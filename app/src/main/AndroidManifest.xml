<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.aakarsh.madassign1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="info.aakarsh.madassign1.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/vit"
        android:label="@string/app_name"
        android:roundIcon="@drawable/vit"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="info.aakarsh.madassign1.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="info.aakarsh.madassign1.MainActivity"></activity>
    </application>

</manifest>